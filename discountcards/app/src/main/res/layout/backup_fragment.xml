<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.backup.BackupFragment">

    <Button
        android:id="@+id/local_export_csv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Local Export CSV"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.127" />

    <Button
        android:id="@+id/local_export_json"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Local Export JSON"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/local_export_csv"
        app:layout_constraintVertical_bias="0.036" />

    <Button
        android:id="@+id/local_import_csv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Local import csv"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/local_export_json"
        app:layout_constraintVertical_bias="0.032" />

    <Button
        android:id="@+id/local_import_json"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Local import json"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/local_import_csv"
        app:layout_constraintVertical_bias="0.095" />

    <Button
        android:id="@+id/online_backup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/online_backup"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/local_import_json"
        app:layout_constraintVertical_bias="0.186" />

    <Button
        android:id="@+id/restore_backup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/restore_backup"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/online_backup"
        app:layout_constraintVertical_bias="0.075" />


</androidx.constraintlayout.widget.ConstraintLayout>